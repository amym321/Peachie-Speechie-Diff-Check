<!-- appHQ : SlideCart Edits -->
<style>
    .customEmptyCart {
        display: block !important
    }

    .customEmptyCart__innerContent {
        padding: 1.2rem;
    }

    #slidecarthq .empty {
        color: #868785;
        position: absolute;
        width: 100%;
        top: 91px;
        bottom: 0;
    }

    .customEmptyCart__headerTitle {
        font-size: 1.7rem;
        font-weight: 700;
        color: black;
    }

    .customEmptyCart__headerLead {
        font-size: 1.1rem;
        font-weight: 400;
        color: black;
      padding-bottom: 30px;
    }

    .customEmptyCart__buttons {
        display: flex !important;
        gap: 1rem;
        flex-wrap: wrap;
        margin-top: 1.2rem;
    }

    a.customEmptyCart__button {
        display: block;
        padding: 0.5rem 1rem;
        background: #67b857;
        width: 100%;
        border-radius: 25px;
        color: #fff;
        text-transform: uppercase;
        font-weight: 700;
        letter-spacing: 1px;
      background-color: black;
}
a.customEmptyCart__button:hover
{ background-color: #67B857;
  transition: all 300ms ease;
    }

    #slidecarthq .quantity-selector button:first-of-type {
        border-top-left-radius: 50px;
        border-bottom-left-radius: 50px;
    }

    #slidecarthq .quantity-selector button:last-of-type {
        border-top-right-radius: 50px;
        border-bottom-right-radius: 50px;
    }

    #slidecarthq .upsell-add button, #slidecarthq .upsell-options select {
        border-radius: 50px !important;
    }

    #slidecarthq .footer .button, #slidecarthq .upsell-options button {
        border-radius: 55px !important;
    }
</style>
<!-- END appHQ : SlideCart Edits -->
  
